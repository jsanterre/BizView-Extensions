<?xml version="1.0" encoding="utf-8"?>
<extension 	type="module"	version="2.5"	client="site"	method="upgrade">
	<name>BizView Map</name>
	<author>Julie Santerre</author>
	<creationDate>February 2012</creationDate>
	<description>Module to add a responsive Google Map</description>
	<files>
		<filename module="mod_map">mod_map.php</filename>
		<folder>tmpl</folder>
		<folder>css</folder>
		<filename>index.html</filename>		
		<filename>mod_map.xml</filename>
	</files>
	<languages>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field type="text" name="address" default="" label="Address" />
			</fieldset>
		</fields>
	</config>
</extension>
